<?xml version="1.1"?>

<!--F-104C tachometer-->

<PropertyList>

<path>tach.ac</path>

<!--***rotations***-->

<animation>
	<type>rotate</type>
	<object-name>needle-1</object-name>
	<property>/engines/engine[0]/n2</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>100</ind><dep>270</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>rotate</type>
	<object-name>needle-2</object-name>
	<property>/engines/engine[0]/n2</property>
	<factor>36</factor>
	<center>
		<x-m>0</x-m>
		<y-m>-0.008913</y-m>
		<z-m>0.009794</z-m>
	</center>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<!--  LoD  -->

<animation> 
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>