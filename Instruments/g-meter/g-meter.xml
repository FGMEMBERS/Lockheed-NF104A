<?xml version="1.0"?>

<!--F-104C g-meter-->

<PropertyList>

<path>g-meter.ac</path>

<!--***rotations***-->

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>/accelerations/pilot/G</property>
	<interpolation>
		<entry><ind>-3</ind><dep>-67.5</dep></entry>
		<entry><ind>-2</ind><dep>-45</dep></entry>
		<entry><ind>-1</ind><dep>-22.5</dep></entry>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1</ind><dep>22.5</dep></entry>
		<entry><ind>2</ind><dep>45</dep></entry>
		<entry><ind>3</ind><dep>67.5</dep></entry>
		<entry><ind>4</ind><dep>90</dep></entry>
		<entry><ind>5</ind><dep>112.5</dep></entry>
		<entry><ind>6</ind><dep>135</dep></entry>
		<entry><ind>7</ind><dep>157.5</dep></entry>
		<entry><ind>8</ind><dep>180</dep></entry>
		<entry><ind>9</ind><dep>202.5</dep></entry>
		<entry><ind>10</ind><dep>225</dep></entry>
	</interpolation>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</center> 
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>rotate</type>
	<object-name>needle-min</object-name>
	<property>/accelerations/pilot/G-min</property>
	<interpolation>
		<entry><ind>-3</ind><dep>-67.5</dep></entry>
		<entry><ind>-2</ind><dep>-45</dep></entry>
		<entry><ind>-1</ind><dep>-22.5</dep></entry>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1</ind><dep>22.5</dep></entry>
		<entry><ind>2</ind><dep>45</dep></entry>
		<entry><ind>3</ind><dep>67.5</dep></entry>
		<entry><ind>4</ind><dep>90</dep></entry>
		<entry><ind>5</ind><dep>112.5</dep></entry>
		<entry><ind>6</ind><dep>135</dep></entry>
		<entry><ind>7</ind><dep>157.5</dep></entry>
		<entry><ind>8</ind><dep>180</dep></entry>
		<entry><ind>9</ind><dep>202.5</dep></entry>
		<entry><ind>10</ind><dep>225</dep></entry>
	</interpolation>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</center> 
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>rotate</type>
	<object-name>needle-max</object-name>
	<property>/accelerations/pilot/G-max</property>
	<interpolation>
		<entry><ind>-3</ind><dep>-67.5</dep></entry>
		<entry><ind>-2</ind><dep>-45</dep></entry>
		<entry><ind>-1</ind><dep>-22.5</dep></entry>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1</ind><dep>22.5</dep></entry>
		<entry><ind>2</ind><dep>45</dep></entry>
		<entry><ind>3</ind><dep>67.5</dep></entry>
		<entry><ind>4</ind><dep>90</dep></entry>
		<entry><ind>5</ind><dep>112.5</dep></entry>
		<entry><ind>6</ind><dep>135</dep></entry>
		<entry><ind>7</ind><dep>157.5</dep></entry>
		<entry><ind>8</ind><dep>180</dep></entry>
		<entry><ind>9</ind><dep>202.5</dep></entry>
		<entry><ind>10</ind><dep>225</dep></entry>
	</interpolation>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</center> 
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<!--***reset***-->

<animation>
	<type>pick</type>
	<object-name>reset-knob</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>property-assign</command>
			<property>/controls/g-meter/reset</property>
			<value>1</value>
		</binding>
	</action>
</animation>

<!-- *** LoD *** -->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>
