<?xml version="1.0"?>

<!--F-104C exhaust gas temperature indicator-->

<PropertyList>

<path>egt.ac</path>

<!--***rotation***-->

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>/engines/engine[0]/egt_degc</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1000</ind><dep>250</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<!-- *** LoD *** -->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>