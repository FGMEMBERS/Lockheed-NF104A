<?xml version="1.0"?>

<!--F-104C external fuel indicator-->

<PropertyList>

<path>fuel2.ac</path>

<!--***rotation***-->

<animation>
	<type>rotate</type>
	<object-name>left-needle</object-name>
	<property>/consumables/fuel/tank[1]/level-lb</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1400</ind><dep>280</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
		</axis>
</animation>

<animation>
	<type>rotate</type>
	<object-name>right-needle</object-name>
	<property>/consumables/fuel/tank[2]/level-lb</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1400</ind><dep>280</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
		</axis>
</animation>

<!-- *** LoD *** -->

<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
</animation>

</PropertyList>