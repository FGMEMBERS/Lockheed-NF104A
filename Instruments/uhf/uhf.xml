<?xml version="1.0" encoding="UTF-8"?>

<!--***UHF repeater***-->

<PropertyList>

 <path>uhf.ac</path>

<!--***digits***-->

<animation>
	<type>textranslate</type>
	<object-name>num1</object-name>
	<property>instrumentation/comm[0]/frequencies/selected-mhz</property>
	<factor>0.001</factor>
	<step>100</step>
	<axis>
		<x>0</x>
		<y>-1</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>textranslate</type>
	<object-name>num2</object-name>
	<property>instrumentation/comm[0]/frequencies/selected-mhz</property>
	<factor>0.01</factor>
	<step>10</step>
	<axis>
		<x>0</x>
		<y>-1</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>textranslate</type>
	<object-name>num3</object-name>
	<property>instrumentation/comm[0]/frequencies/selected-mhz</property>
	<factor>0.1</factor>
	<step>1</step>
	<axis>
		<x>0</x>
		<y>-1</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>textranslate</type>
	<object-name>num4</object-name>
	<property>instrumentation/comm[0]/frequencies/selected-mhz</property>
	<factor>1</factor>
	<step>.1</step>
	<axis>
		<x>0</x>
		<y>-1</y>
		<z>0</z>
	</axis>
</animation>

<!--  LoD  -->

<animation> 
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>

