<?xml version="1.1"?>

<!--***F-104C vertical velocity indicator***-->

<PropertyList>

<path>vvi.ac</path>

<!--***rotation***-->

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>fdm/jsbsim/velocities/v-down-fps</property>
	<interpolation>
		<entry><ind>100</ind><dep>-170.0</dep></entry>
		<entry><ind>83.3333</ind><dep>-152.5</dep></entry>
		<entry><ind>66.6667</ind><dep>-135.0</dep></entry>
		<entry><ind>50</ind><dep>-107.5</dep></entry>
		<entry><ind>33.3333</ind><dep>-80.0</dep></entry>
		<entry><ind>25</ind><dep>-62.5</dep></entry>
		<entry><ind>16.6667</ind><dep>-45.0</dep></entry>
		<entry><ind>0.0</ind><dep>0.0</dep></entry>
		<entry><ind>-16.6667</ind><dep>45.0</dep></entry>
		<entry><ind>-25</ind><dep>62.5</dep></entry> 
		<entry><ind>-33.3333</ind><dep>80.0</dep></entry> 
		<entry><ind>-50</ind><dep>107.5</dep></entry>
		<entry><ind>-66.6667</ind><dep>135.0</dep></entry>
		<entry><ind>-83.3333</ind><dep>152.5</dep></entry>
		<entry><ind>-100</ind><dep>170</dep></entry>
	</interpolation>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</center> 
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<!--***LoD***-->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>
