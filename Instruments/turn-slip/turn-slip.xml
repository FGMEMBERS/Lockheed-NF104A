<?xml version="1.1"?>

<!--F-104C turn-slip indicator-->

<PropertyList>

<path>turn-slip.ac</path>

<!--***rotations***-->

<animation>
	<type>rotate</type>
	<object-name>slip-ball</object-name>
	<property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
	<interpolation>
		<entry><ind>-2.5</ind><dep>-25</dep></entry>
		<entry><ind>2.5</ind><dep>25</dep></entry>
	</interpolation>
	<min>-2.5</min>
	<max>2.5</max>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0.029871</z-m>
	</center>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>orientation/yaw-rate-degps</property>
	<interpolation>
		<entry><ind>-1.5</ind><dep>-15</dep></entry>
		<entry><ind>1.5</ind><dep>15</dep></entry>
	</interpolation>
	<min>-1.5</min>
	<max>1.5</max>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>-.006641</z-m>
	</center>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

<!-- *** LoD *** -->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>


