<?xml version="1.1"?>

<!--***F-104C radar***-->

<PropertyList>

 <path>radar.ac</path>
   
<!--***attitude reference***-->

<animation>
	<type>rotate</type>
	<object-name>attitude-ref</object-name>
	<property>orientation/roll-deg</property>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</center>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>  

<!--***Sweep***-->

<animation>
	<type>translate</type>
	<object-name>sweep</object-name>
	<property>sim/multiplay/generic/float[0]</property>
	<interpolation>
		<entry><ind>-1</ind><dep>-0.04372</dep></entry>
		<entry><ind>1</ind><dep>0.04372</dep></entry>
    </interpolation>
	<center>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
	</axis>
</animation>

<!--  LoD  -->

<animation> 
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>


