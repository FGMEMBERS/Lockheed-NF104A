<?xml version="1.0"?>

<!--F-104C hydraulic pressure indicator, system 2-->

<PropertyList>

<path>hyd-press.ac</path>

<!--***rotations***-->

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>/engines/engine[0]/running</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1</ind><dep>240</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/surface-positions/flap-pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>.001</ind><dep>-40</dep></entry>
			<entry><ind>.389</ind><dep>-40</dep></entry>
			<entry><ind>.29</ind><dep>0</dep></entry>
			<entry><ind>.301</ind><dep>-40</dep></entry>
			<entry><ind>.999</ind><dep>-40</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/gear/gear[0]/position-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>.001</ind><dep>-40</dep></entry>
			<entry><ind>.999</ind><dep>-40</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/surface-positions/speedbrake-pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>.001</ind><dep>-20</dep></entry>
			<entry><ind>.999</ind><dep>-20</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/controls/gear/brake-left</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>.001</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>-10</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/controls/gear/brake-right</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>.001</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>-10</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- *** LoD *** -->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>