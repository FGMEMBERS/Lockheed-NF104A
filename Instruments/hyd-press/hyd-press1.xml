<?xml version="1.0"?>

<!--F-104C hydraulic pressure indicator, system 1-->

<PropertyList>

<path>hyd-press.ac</path>

<!--***rotation***-->

<animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>/engines/engine[0]/running</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1</ind><dep>240</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

	<!-- *** LoD *** -->

<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>5</max-m>
</animation>

</PropertyList>