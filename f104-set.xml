  <sim>
    <description>Lockheed F-104</description>
    <author>Erik Hofman (FDM), Emmanuel BARANGER, Kent Esbenshade (3D)</author>

    <model>
      <path>Aircraft/Lockheed-NF104A/Models/F-104C.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>
  </sim>

  <nasal>
