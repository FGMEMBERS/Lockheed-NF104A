<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>asi.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>cache</object-name>
    <object-name>face</object-name>
    <object-name>mach</object-name>
    <object-name>appch</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>mach</object-name>
    <object-name>appch</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mach</object-name>
    <property>velocities/mach</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   0 </dep></entry>
      <entry><ind> 0.4 </ind><dep>  20 </dep></entry>
      <entry><ind> 0.6 </ind><dep>  60 </dep></entry>
      <entry><ind> 0.8 </ind><dep> 100 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 140 </dep></entry>
      <entry><ind> 1.4 </ind><dep> 220 </dep></entry>
      <entry><ind> 1.6 </ind><dep> 260 </dep></entry>
      <entry><ind> 1.8 </ind><dep> 300 </dep></entry>
      <entry><ind> 2.0 </ind><dep> 340 </dep></entry>
      <entry><ind> 2.1 </ind><dep> 360 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <object-name>mach</object-name>
    <property>velocities/airspeed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind> 150 </ind><dep>  22 </dep></entry>
      <entry><ind> 200 </ind><dep>  70 </dep></entry>
      <entry><ind> 300 </ind><dep> 122 </dep></entry>
      <entry><ind> 400 </ind><dep> 163 </dep></entry>
      <entry><ind> 500 </ind><dep> 197 </dep></entry>
      <entry><ind> 600 </ind><dep> 222 </dep></entry>
      <entry><ind> 700 </ind><dep> 246 </dep></entry>
      <entry><ind> 850 </ind><dep> 291 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>appch</object-name>
    <property>velocities/approach-speed</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind> 150 </ind><dep>  22 </dep></entry>
      <entry><ind> 200 </ind><dep>  70 </dep></entry>
      <entry><ind> 300 </ind><dep> 122 </dep></entry>
      <entry><ind> 400 </ind><dep> 163 </dep></entry>
      <entry><ind> 500 </ind><dep> 197 </dep></entry>
      <entry><ind> 600 </ind><dep> 222 </dep></entry>
      <entry><ind> 700 </ind><dep> 246 </dep></entry>
      <entry><ind> 850 </ind><dep> 291 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

