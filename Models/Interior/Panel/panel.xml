<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>centerpanel</object-name>
    <object-name>leftpanel</object-name>
    <object-name>rightpanel</object-name>
    <object-name>background</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- AI -->
  <model>
    <path>Aircraft/Lockheed-NF104A/Models/Interior/Panel/Instruments/ai/ai.xml</path>
    <offsets>
      <x-m> -0.489 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.291 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>   0 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- ASI -->
  <model>
    <path>Aircraft/Lockheed-NF104A/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m> -0.488 </x-m>
      <y-m>  0.105 </y-m>
      <z-m>  0.301 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>   0 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- ALT -->
  <model>
    <path>Aircraft/Lockheed-NF104A/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m> -0.488 </x-m>
      <y-m>  0.105 </y-m>
